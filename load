-- Roblox Executor Cute Loading Screen (10 minutes)

local duration = 600 -- 10 minutes
local totalAssets = 6750
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Screen GUI
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "ChibiLoadingScreen"
screenGui.Parent = playerGui

-- Main Frame (with transparent background)
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 300, 0, 250)
frame.Position = UDim2.new(0.5, -150, 0.5, -125)
frame.BackgroundTransparency = 1 -- hide original solid background
frame.BorderSizePixel = 0
frame.Parent = screenGui

-- Moving Striped Background
local stripesBg = Instance.new("ImageLabel")
stripesBg.Size = UDim2.new(2, 0, 2, 0) -- bigger than frame for smooth scrolling
stripesBg.Position = UDim2.new(0, 0, 0, 0)
stripesBg.BackgroundTransparency = 1
stripesBg.Image = "rbxassetid://INSERT_STRIPES_IMAGE_ID" -- replace with repeating stripes asset
stripesBg.ScaleType = Enum.ScaleType.Tile
stripesBg.Parent = frame

-- Animate stripes (diagonal movement)
spawn(function()
    while true do
        stripesBg.Position = stripesBg.Position + UDim2.new(-0.002, 0, -0.002, 0)
        if stripesBg.Position.X.Scale <= -1 then
            stripesBg.Position = UDim2.new(0, 0, 0, 0)
        end
        wait(0.03)
    end
end)

-- Fade In / Fade Out
local function fadeIn(gui, time)
    for i = 0, 1, 0.02 do
        gui.BackgroundTransparency = 1 - (0.4 * i) -- slightly visible stripes
        wait(time * 0.02)
    end
end

local function fadeOut(gui, time)
    for i = 0, 1, 0.02 do
        gui.BackgroundTransparency = 0.6 + (0.4 * i)
        wait(time * 0.02)
    end
end

-- Loading Text (Top)
local textLabel = Instance.new("TextLabel")
textLabel.Size = UDim2.new(0.9, 0, 0.2, 0)
textLabel.Position = UDim2.new(0.5, 0, 0.05, 0)
textLabel.AnchorPoint = Vector2.new(0.5, 0)
textLabel.BackgroundTransparency = 1
textLabel.Text = "Loading..."
textLabel.Font = Enum.Font.FredokaOne
textLabel.TextColor3 = Color3.fromRGB(255, 105, 180)
textLabel.TextScaled = true
textLabel.TextWrapped = true
textLabel.Parent = frame

-- Circle Logo (Centered Below Text)
local logo = Instance.new("ImageLabel")
logo.Size = UDim2.new(0, 80, 0, 80)
logo.Position = UDim2.new(0.5, 0, 0.35, 0)
logo.AnchorPoint = Vector2.new(0.5, 0.5)
logo.BackgroundTransparency = 1
logo.Image = "rbxassetid://INSERT_YOUR_ASSET_ID_HERE" -- replace with your logo
logo.Parent = frame

-- Asset Counter (Below Logo)
local assetLabel = Instance.new("TextLabel")
assetLabel.Size = UDim2.new(0.8, 0, 0.15, 0)
assetLabel.Position = UDim2.new(0.5, 0, 0.55, 0)
assetLabel.AnchorPoint = Vector2.new(0.5, 0)
assetLabel.BackgroundTransparency = 1
assetLabel.Text = "0/"..totalAssets
assetLabel.Font = Enum.Font.FredokaOne
assetLabel.TextColor3 = Color3.fromRGB(255, 105, 180)
assetLabel.TextScaled = true
assetLabel.Parent = frame

-- Progress Bar (Below Asset Label)
local progressBg = Instance.new("Frame")
progressBg.Size = UDim2.new(0.8, 0, 0, 20)
progressBg.Position = UDim2.new(0.1, 0, 0.7, 0)
progressBg.BackgroundColor3 = Color3.fromRGB(255, 200, 220)
progressBg.BorderSizePixel = 0
progressBg.Parent = frame

local progressFill = Instance.new("Frame")
progressFill.Size = UDim2.new(0, 0, 1, 0)
progressFill.BackgroundColor3 = Color3.fromRGB(255, 105, 180)
progressFill.BorderSizePixel = 0
progressFill.Parent = progressBg

-- Fade in frame at start
fadeIn(frame, 1)

-- Animate Progress and Asset Count
local startTime = os.clock()
while true do
    local elapsed = os.clock() - startTime
    if elapsed > duration then break end
    local progress = elapsed / duration
    progressFill.Size = UDim2.new(progress, 0, 1, 0)
    local assetsLoaded = math.floor(progress * totalAssets)
    assetLabel.Text = assetsLoaded.."/"..totalAssets
    wait(0.1)
end

-- Ensure full completion
progressFill.Size = UDim2.new(1, 0, 1, 0)
assetLabel.Text = totalAssets.."/"..totalAssets
textLabel.Text = "Done!"
wait(2)
fadeOut(frame, 1)
screenGui:Destroy()
